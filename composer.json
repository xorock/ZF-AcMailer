{
    "name" : "acelaya/zf2-acmailer",
    "description" : "Mail sending module for Zend Framework 2",
    "type" : "library",
    "authors" : [
        {
            "name" : "Alejandro Celaya Alastru√©",
            "email" : "alejandro@alejandrocelaya.com",
            "homepage" : "http://www.alejandrocelaya.com",
            "role" : "Developer"
        }
    ],
    "keywords" : [
        "zf2",
        "module",
        "mail",
        "php",
        "zend"
    ],
    "homepage" : "https://github.com/acelaya/ZF2-AcMailer",
    "license" : "MIT",
    "require" : {
        "php" : "^5.6|^7.0",
        "zendframework/zend-servicemanager": "^3.0",
        "zendframework/zend-mail": "^2.6",
        "zendframework/zend-view": "^2.6",
        "zendframework/zend-eventmanager": "^3.0",
        "zendframework/zend-mime": "^2.6",
        "zendframework/zend-stdlib": "^3.0",
        "zendframework/zend-console": "^2.6",
        "zendframework/zend-modulemanager": "^2.6",
        "zendframework/zend-mvc": "^3.0",
        "zendframework/zend-mvc-console": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0",
        "squizlabs/php_codesniffer": "~2.0",
        "zendframework/zend-json": "^3.0"
    },
    "autoload" : {
        "psr-4" : {
            "AcMailer\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "AcMailerTest\\" : "test/"
        }
    },
    "extra": {
        "zf": {
            "module": "AcMailer",
            "config-provider": "AcMailer\\ConfigProvider"
        }
     }
}
